\n    <my-header :cartItemCount=\"cartItemCount\"></my-header>\n\n    <div class=\"row\">\n      <div class=\"col-md-10 col-md-offset-1\">\n        <div class=\"panel panel-info\">\n          <div class=\"panel-heading\">Касса зоомагазина</div>\n          <div class=\"panel-body\">\n            <div class=\"form-group\">\n              <div class=\"col-md-12\">\n                <h4><strong>Введите вашу информацию</strong></h4>\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-6\">\n                <strong>Имя:</strong>\n                <input v-model.trim=\"order.firstName\" class=\"form-control\" />\n              </div>\n              <div class=\"col-md-6\">\n                <strong>Фамилия:</strong>\n                <input v-model.trim=\"order.lastName\" class=\"form-control\" />\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-12\"><strong>Адрес:</strong></div>\n              <div class=\"col-md-12\">\n                <input v-model.trim=\"order.address\" class=\"form-control\" />\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-12\"><strong>Город:</strong></div>\n              <div class=\"col-md-12\">\n                <input v-model.trim=\"order.city\" class=\"form-control\" />\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-2\">\n                <strong>регион:</strong>\n                <select v-model=\"order.region\" class=\"form-control\">\n                  <option disabled value=\"\">Регион</option>\n                  <option v-for=\"(region, key) in regions\" v-bind:value=\"region\">\n                    {{ key }}\n                  </option>\n                </select>\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-6 col-md-offset-4\">\n                <strong>Индекс:</strong>\n                <input\n                  v-model.number=\"order.code\"\n                  class=\"form-control\"\n                  type=\"number\"\n                />\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-6 boxes\">\n                <input\n                  type=\"checkbox\"\n                  id=\"gift\"\n                  value=\"true\"\n                  v-bind:true-value=\"order.sendGift\"\n                  v-bind:false-value=\"order.dontSendGift\"\n                  v-model=\"order.gift\"\n                />\n                <label for=\"gift\">Упаковка ввиде подарка</label>\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-6 boxes\">\n                <input\n                  type=\"radio\"\n                  id=\"home\"\n                  v-bind:value=\"order.home\"\n                  v-model=\"order.method\"\n                />\n                <label for=\"home\">Самовызов</label>\n                <input\n                  type=\"radio\"\n                  id=\"business\"\n                  v-bind:value=\"order.business\"\n                  v-model=\"order.method\"\n                />\n                <label for=\"business\">Доставка</label>\n              </div>\n            </div>\n\n            <div class=\"form-group\">\n              <div class=\"col-md-6\">\n                <button\n                  type=\"submit\"\n                  class=\"btn btn-primary submit\"\n                  v-on:click=\"submitForm\"\n                >\n                  Place Order\n                </button>\n              </div>\n            </div>\n\n            <div class=\"col-md-12 verify\">\n              <pre>\n                        First Name: {{ order.firstName }}\n                        Last Name: {{ order.lastName }}\n                        Address: {{ order.address }}\n                        City: {{ order.city }}\n                        code: {{ order.code }}\n                        Region: {{ order.region }}\n                        Method: {{ order.method }}\n                        Gift: {{ order.gift }}\n              </pre>\n            </div>\n            <!-- end of col-md-12 verify-->\n          </div>\n          <!--end of panel-body-->\n        </div>\n        <!--end of panel panel-info-->\n      </div>\n      <!--end of col-md-10 col-md-offset-1-->\n    </div>\n    <!--end of row-->\n  </div>\n</template>\n\n<script>\nimport MyHeader from \"./Header.vue\";\nexport default {\n  name: \"Form\",\n  props: [\"cartItemCount\"],\n  data() {\n    return {\n      regions: {\n        Chel: \"Челябинска\",\n        Mos: \"Московская\",\n        YaAO: \"Ямало-немецкий АО\"\n      },\n      order: {\n        firstName: \"\",\n        lastName: \"\",\n        address: \"\",\n        city: \"\",\n        code: \"\",\n        region: \"\",\n        method: \"Самовызов\",\n        business: \"Доставка\",\n        home: \"Домашний адрес\",\n        gift: \"\",\n        sendGift: \"Как подарок\",\n        dontSendGift: \"Без подарка\"\n      }\n    };\n  },\n  components: { MyHeader },\n  methods: {\n    submitForm() {\n      alert(\"Submitted\");\n    }\n  }\n};\n</script>\n\n<style ограниченный></style>\n"],"sourceRoot":"src/components"}]}